        app:layout_constraintRight_toLeftOf="@+id/previousdistance"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/previousdistance" />

    <TextView
